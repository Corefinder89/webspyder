{
  "Tag Management System":[
    {
      "name":"Google Tag Manager",
      "condition":{
        "$operator": "$or",
        "conditions":[
          {
            "$location": "path",
            "$condition": "contains",
            "$value": "gtm.js",
            "$case": "ignorecase"
          }
        ]
      }
    },
    {
      "name":"SuperTag",
      "condition":{
        "$operator": "$or",
        "conditions":[
          {
            "$location": "path",
            "$condition": "contains",
            "$value": "supertag-code-",
            "$case": "ignorecase"
          }
        ]
      }
    },
    {
      "name":"Tealium",
